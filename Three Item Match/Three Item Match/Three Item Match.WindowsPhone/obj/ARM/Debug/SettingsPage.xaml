<Page
    x:Class="Three_Item_Match.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Three_Item_Match"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" RequestedTheme="Dark">

    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="{StaticResource BGColor}"/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto" x:Name="BackRow"/>
        </Grid.RowDefinitions>
        <Button x:ConnectionId='1' Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5"                         >Go Back</Button>
    </Grid>
</Page>

